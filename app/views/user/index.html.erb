<div class="container">
  <h1>Hello, <%=@user.name%></h1>

  <div class="flaticon-factory6"></div>
  <div class="flaticon-map32"></div>

  <ul id="location">
    <% @unique_locations.each do |locale| %>
      <li><a href="#"><%= locale %></a></li>
